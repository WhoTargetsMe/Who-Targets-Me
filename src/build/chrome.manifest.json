{
	"version": "1.2.50",
	"name": "__MSG_appName__",
	"manifest_version": 2,
	"description": "__MSG_appDesc__",
	"default_locale": "en",
	"browser_action": {
		"default_title": "__MSG_appName__",
		"default_icon": "wtm_logo_128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://msih3yho6h.execute-api.eu-central-1.amazonaws.com/dev/*",
				"https://www.facebook.com/*",
				"http://localhost:3000/*",
				"https://results.whotargets.me/*",
				"https://results-dev.whotargets.me/*",
				"https://wtm-api.whotargets.me/*"
			],
			"js": ["daemon/index.js", "daemon/collect.js"]
		}
	],
	"background": {
		"scripts": ["daemon/index.js"]
	},
	"permissions": [
		"<all_urls>",
		"tabs",
		"storage"
	],
	"icons": {
		"128": "wtm_logo_128.png"
	},
	"web_accessible_resources": [ "daemon/overload.js", "daemon/overload.js.map" ]
}
