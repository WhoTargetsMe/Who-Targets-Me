{
	"version": "1.2.36",
	"name": "__MSG_appName__",
	"manifest_version": 2,
	"description": "__MSG_appDesc__",
	"default_locale": "en",
	"browser_action": {
		"default_title": "__MSG_appName__",
		"default_popup": "frontend/index.html",
		"default_icon": "logo-128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://msih3yho6h.execute-api.eu-central-1.amazonaws.com/dev/*", "https://www.facebook.com/*"],
			"js": ["daemon/index.js", "daemon/collect.js"]
		}, {
			"matches": ["*://whotargetsme.voxpoplabs.com/*"],
			"js": ["daemon/index.js", "daemon/voxpop.js"],
			"all_frames": true
		}
	],
	"background": {
		"scripts": ["daemon/index.js"]
	},
	"permissions": [
		"<all_urls>",
		"contextMenus",
		"tabs",
		"storage",
		"notifications"
	],
	"icons": {
		"128": "logo-128.png"
	},
	"web_accessible_resources": [ "daemon/overload.js" ]
}
