{
	"version": "1.2.50",
	"name": "__MSG_appName__",
	"manifest_version": 2,
	"description": "__MSG_appDesc__",
	"default_locale": "en",
	"browser_action": {
		"default_icon": "wtm_logo_128.png"
	},
	"permissions": [
		"storage",
		"*://facebook.com/*",
		"*://www.facebook.com/*",
		"*://*.whotargets.me/*",
		"*://whotargets.me/*",
		"*://localhost:3000/*",
		"tabs"
	],
	"content_scripts": [
		{
			"matches": [
				"*://facebook.com/*",
				"*://*.facebook.com/*",
				"*://localhost:3000/*",
				"*://results.whotargets.me/*",
				"*://results-dev.whotargets.me/*",
				"*://wtm-api.whotargets.me/*"
			],
			"js": ["daemon/index.js", "daemon/collect.js"]
		}
	],
	"background": {
		"scripts": ["daemon/index.js"]
	},
	"icons": {
		"128": "wtm_logo_128.png"
	},
	"web_accessible_resources": [ "daemon/overload.js" ]
}
